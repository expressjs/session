{
  "compilerOptions": {
    "strict": true,
    "noImplicitAny": true,
    "strictFunctionTypes": true,
    "strictPropertyInitialization": true,
    "strictBindCallApply": true,
    "noImplicitThis": true,
    "noImplicitReturns": true,
    "alwaysStrict": true,
    "esModuleInterop": true,
    "checkJs": true,
    "allowJs": true,
    "declaration": true,
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "node",
    "outDir": "types"
  },
  "include": [
    "./session/**/*.ts",
    // "./session/**/*.js",
    "./session/cookie.js",
    "./session/session.js",
    // "./index.js"
  ],
  "verbose": true
}
